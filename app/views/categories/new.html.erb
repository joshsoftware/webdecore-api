<h1>Add Primary Category</h1>
<%= form_with scope: :category, url: categories_path,local: true do |form| %>
<p>
<%= form.label :Primary_Category_Name %><br>
<%= form.text_field :category_name %>
</p>
<p>
<%= form.label :Primary_Category_Description %><br>
<%= form.text_field :category_description %>
</p>


<p>
<%= form.label :Primary_Category_Image %><br>
<%= form.file_field :picture %>
</p>
<p>
<%= form.submit %>
</p>
<% end %>
